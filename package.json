{
  "name": "docker-bacpac-manager",
  "displayName": "Docker SQL Bacpac Tool",
  "description": "Import and Export BACPAC files to/from SQL Server Docker containers",
  "version": "1.4.0",
  "publisher": "jorgedorio",
  "engines": {
    "vscode": "^1.80.0"
  },
  "icon": "icon.png",
  "categories": [
    "Other"
  ],
  "keywords": [
    "sql server",
    "docker",
    "bacpac",
    "database",
    "mssql"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "docker-sql-import.importBacpac",
        "title": "SQL: Import Bacpac to Docker"
      },
      {
        "command": "docker-sql-import.exportBacpac",
        "title": "SQL: Export Bacpac from Docker"
      }
    ]
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/node": "^16.x"
  },
  "dependencies": {
    "dockerode": "^4.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JorgeDorio/docker-bacpac-manager"
  }
}